    <script>
    const hamburger = document.querySelector(".hamburger");
const navLinks = document.querySelector(".nav-links-blink");
const links = document.querySelectorAll(".nav-links-blink li");

hamburger.addEventListener("click", () => {
  navLinks.classList.toggle("open");
  links.forEach(link => {
    link.classList.toggle("fade");
  });
});
    </script>


